w:Toggle('Auto Sell', {flag = "AutoSell"})
spawn( 
    function()
        while wait() do
          if w.flags.AutoSell then
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ "SellMuscle"})
end
end
end)   

local w = library:CreateWindow('Other')
w:Section('Misc')
local b = w:Button("AFK", function()
   repeat wait() until game.Players.LocalPlayer.Character
    url = "https://raw.githubusercontent.com/shravan-alt/Scripts/main/Anti-AFK"
    loadstring(game:HttpGet(url))()
end)

w:Section('Tool')
local b = w:Button("Tool Farm", function()
while true do
game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):Activate()
wait()
end
end)

w:Section('Auto lift x100 increase ping')
w:Section('Equip Weight to use "Tool Farm"')
w:Section('Right Ctrl to hide')
