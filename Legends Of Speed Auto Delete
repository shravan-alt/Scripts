while true do
local args = {
    [1] = "sellPet",
    [2] = game:GetService("Players").LocalPlayer.petsFolder.Omega:FindFirstChild("Dark Soul Birdie")
}

game:GetService("ReplicatedStorage").rEvents.sellPetEvent:FireServer(unpack(args))

